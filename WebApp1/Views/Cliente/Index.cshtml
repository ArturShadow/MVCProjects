@using WebApp1.Models
@model List<ClienteCLS>
@{
    ViewBag.Title = "Index";
}

<h1>Lista de clientes</h1>

<table class="table table-dark">
    <thead>
        <tr>
            <td>@Html.LabelFor(p => Model[0].IdCliente)</td>
            <td>@Html.LabelFor(p => Model[0].Nombre)</td>
            <td>@Html.LabelFor(p => Model[0].APaterno)</td>
            <td>@Html.LabelFor(p => Model[0].AMaterno)</td>
            <td>@Html.LabelFor(p => Model[0].Email)</td>
            <td>@Html.LabelFor(p => Model[0].Direccion)</td>
            @* <td>@Html.LabelFor(p => Model[0].Sexo)</td> *@
            <td>@Html.LabelFor(p => Model[0].Telefono)</td>
        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model)
        {
            <tr>
                <td>@item.IdCliente</td>
                <td>@item.Nombre</td>
                <td>@item.APaterno</td>
                <td>@item.AMaterno</td>
                <td>@item.Email</td>
                <td>@item.Direccion</td>
                @* <td>@item.Sexo</td> *@
                <td>@item.Telefono</td>
            </tr>
        }
    </tbody>
</table>